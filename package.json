{
  "name": "babel-plugin-inline-debugger",
  "version": "0.0.0-auto",
  "description": "A Babel plugin for selective expression monitoring and debugging in TypeScript projects",
  "main": "dist/babel-plugin-inline-debugger.js",
  "types": "dist/babel-plugin-inline-debugger.d.ts",
  "keywords": [
    "babel-plugin",
    "debugging",
    "monitoring",
    "typescript",
    "expression-tracking",
    "development-tools",
    "jest",
    "testing"
  ],
  "author": "Sujimoshi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Sujimoshi/babel-plugin-inline-debugger.git"
  },
  "bugs": {
    "url": "https://github.com/Sujimoshi/babel-plugin-inline-debugger/issues"
  },
  "homepage": "https://github.com/Sujimoshi/babel-plugin-inline-debugger#readme",
  "files": [
    "dist/",
    "src/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:update": "jest --updateSnapshot",
    "prepublishOnly": "npm run build",
    "lint": "echo 'Linting not configured yet'",
    "clean": "rm -f .debug.data.json"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-typescript": "^7.23.0"
  },
  "devDependencies": {
    "@babel/generator": "^7.23.0",
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__traverse": "^7.28.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.4",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
